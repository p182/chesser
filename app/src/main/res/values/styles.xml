<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#003856</item>
        <item name="colorPrimaryDark">#002033</item>
        <item name="colorAccent">#007a9f</item>
        <item name="android:windowBackground">@color/org</item>
        <item name="android:colorBackground">@color/org</item>
    </style>


    <style name="Theme2" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#1565C0</item>
        <item name="colorPrimaryDark">#0D47A1</item>
        <item name="colorAccent">#002033</item>
        <item name="android:windowBackground">@color/background2</item>
        <item name="android:colorBackground">@color/background2</item>
    </style>


    <style name="NoActionBar" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowNoTitle">true</item>
        <item name="colorPrimaryDark">#ba003554</item>
        <!--
        <item name="colorPrimaryDark">#3e67b6</item>
        -->
        <item name="android:windowBackground">@color/org</item>
        <item name="android:colorBackground">@color/org</item>
    </style>

</resources>
